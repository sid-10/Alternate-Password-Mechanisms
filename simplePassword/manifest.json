{
  "manifest_version": 2,

  "name": "XXX",
  "description": "we all are going to die soon so, relax and enjoy !! ",
  "version": "1.0",

   
    
    "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["js/indexjs.js","js/loginjs.js","js/rollups/aes.js"]
  }
], 
    
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "index.html"
  },
    "content_security_policy": "script-src 'self' https://code.jquery.com; object-src 'self'" ,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    
  "permissions": [
   "activeTab"
   ],
 "web_accessible_resources": [
    "login.html"]
}
